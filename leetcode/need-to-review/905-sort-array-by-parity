def sort_array_by_parity(a)
    even = []
    odd = []
    a.each do |n|
        if n % 2 == 0
            even << n
        else
            odd << n
        end
    end
    return even + odd
end
================================================
var sortArrayByParity = function(A) {
    let odd = []
    let even = []
    A.forEach (function(e) {
        if (e % 2 === 0) {
        even.push(e)
    }  else {
        odd.push(e)
    }
               })
   return even.concat(odd)
};
