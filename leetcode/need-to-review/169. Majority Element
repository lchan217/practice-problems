Ruby:
def majority_element(nums)
    obj = Hash.new(0)
    nums.each do |n|
        obj[n] += 1
    end

    answer = Integer
end



def majority_element(nums)
    obj = Hash.new(0)
    nums.each do |n|
        obj[n] += 1
    end
    answer = Integer
    obj.each do |key,value|
        if value > nums.length/2
            answer = key
        end
    end
    answer
end
