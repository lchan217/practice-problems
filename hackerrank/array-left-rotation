function rotLeft(a, d) {
    for(let i=0; i <d; i++){
        a.push(a[0])
        a.shift()
    }
    return a
}

def rotLeft(a, d)
    while d > 0 do 
        element = a.shift()
        a.push(element)
        d -=1
    end
    a
end